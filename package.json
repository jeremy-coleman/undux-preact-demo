{
  "name": "@jeremy-coleman/undux-preact-demo",
  "private": true,
  "scripts": {
    "start1": "node start",
    "start": "webpack serve",
    "pretty": "pprettier --write src/**/*.{js,jsx,ts,tsx}"
  },
  "prettier": {
    "bracketSpacing": false,
    "parser": "typescript",
    "arrowParens": "always",
    "jsxBracketSameLine": false,
    "printWidth": 100,
    "quoteProps": "consistent",
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "none"
  },
  "importSort": {
    ".js, jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "emotion": "11.0.0",
    "feather-icons": "4.28.0",
    "highlight.js": "10.4.0",
    "polished": "4.0.5",
    "preact": "10.5.7",
    "preact-feather": "4.1.0",
    "prismjs": "1.22.0",
    "rxjs": "7.0.0-beta.8"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "7.12.1",
    "webpack-dev-server": "3.11.0",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/core": "7.12.9",
    "@babel/plugin-transform-react-jsx": "7.12.7",
    "@prefresh/babel-plugin": "0.2.2",
    "@prefresh/webpack": "2.2.1",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.0.0",
    "chalk": "4.1.0",
    "express": "4.17.1",
    "@mixer/parallel-prettier": "2.0.0",
    "@types/node": "14.14.10",
    "css-loader": "5.0.1",
    "file-loader": "6.2.0",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "4.5.0",
    "markdown-loader": "6.0.0",
    "prettier": "2.2.1",
    "style-loader": "2.0.0",
    "ts-loader": "8.0.11",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "typescript": "4.1.2",
    "url-loader": "4.1.1",
    "webpack": "5.9.0",
    "webpack-dev-middleware": "4.0.2",
    "webpack-hot-middleware": "2.25.0"
  }
}
